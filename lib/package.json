{
  "name": "@clalarco/vue3-plotly",
  "version": "0.1.1",
  "description": "Vue 3 component for plotly.js. Forked from <https://github.com/VanOord/vue3-plotly>",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/plotly-vue-ts.umd.js",
  "module": "./dist/plotly-vue-ts.es.js",
  "exports": {
    ".": {
      "import": "./dist/plotly-vue-ts.es.js",
      "require": "./dist/plotly-vue-ts.umd.js"
    }
  },
  "types": "./dist/main.d.ts",
  "scripts": {
    "build": "vue-tsc && vite build"
  },
  "peerDependencies": {
    "vue": "3.x"
  },
  "dependencies": {
    "plotly.js-dist": "^2.24.3"
  },
  "devDependencies": {
    "@types/plotly.js-basic-dist": "^1.54.1",
    "@types/plotly.js": "^2.12.24",
    "@vitejs/plugin-vue": "^4.2.3",
    "path": "^0.12.7",
    "rollup-plugin-typescript2": "^0.35.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.3.1",
    "vue-tsc": "^1.8.5"
  }
}
